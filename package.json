{
  "name": "icon-fronted-v2",
  "private": true,
  "version": "2.38.4",
  "description": "User facing front-end web application for ICON (Immunization Connect ONtario)",
  "dependencies": {
    "Base64": "1.0.0",
    "angular": "1.6.4",
    "angular-animate": "1.6.4",
    "angular-bootstrap-affix": "0.2.4",
    "angular-css": "github:castillo-io/angular-css",
    "angular-dynamic-locale": "0.1.32",
    "angular-file-saver": "1.1.3",
    "angular-file-upload": "2.5.0",
    "angular-jwt": "0.0.9",
    "angular-messages": "1.5.7",
    "angular-moment": "1.0.0",
    "angular-scroll": "1.0.0",
    "angular-translate": "2.11.1",
    "angular-translate-interpolation-messageformat": "2.11.1",
    "angular-ui-bootstrap": "2.4.0",
    "angular-ui-mask": "1.8.7",
    "angular-ui-router": "1.0.0-rc.1",
    "angulartics": "1.4.0",
    "blob-util": "1.2.1",
    "crypto-js": "3.1.6",
    "custom-event-polyfill": "0.3.0",
    "json-loader": "0.5.4",
    "lodash": "4.17.4",
    "moment": "2.17.0",
    "ng-device-detector": "3.0.1",
    "object-assign-polyfill": "0.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-angularjs-annotate": "^0.7.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "3.5.0",
    "copy-webpack-plugin": "4.0.1",
    "forever": "0.15.2",
    "http-server": "0.9.0",
    "husky": "^0.14.3",
    "jasmine": "2.5.2",
    "jasmine-bamboo-reporter": "0.0.2",
    "jasmine-core": "2.4.1",
    "jasmine-reporters": "2.2.0",
    "jsdoc": "3.4.0",
    "mocha-jenkins-reporter": "0.3.7",
    "rimraf": "2.5.4",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "standard": {
    "globals": [
      "angular"
    ]
  },
  "scripts": {
    "install-exact": "echo \"Installing ICON front-end application dependencies (exact versions) ...\" && npm install -E",
    "build-cleanup": "echo \"Attempting to remove previous dist folder if it existed ...\" && rimraf -f dist",
    "build-prod": "echo \"Building optimized production version of ICON front-end application ...\" && webpack --config webpack.prod.config.js",
    "build-dev-serve-hot-reload": "webpack-dev-server --config webpack.dev.config.js --public xxsupportphu1.vcap.me:3001 --port 3001 --content-base ./src/",
    "static-serve": "echo \"Serving ICON front-end application statically at http://localhost:3001 ...\" && http-server -a localhost -p 3001 -c-1 ./dist -s",
    "generate-docs": "echo \"Generating documentation for ICON front-end application (check directory /out) ...\" && jsdoc -c ./jsdoc.config.js -r",
    "dev": "npm run build-dev-serve-hot-reload",
    "lab": "echo WARNING! This command is deprecated, instead use: \"> npm run dev\" && npm run dev",
    "dev-serve": "npm run build-dev-serve-hot-reload",
    "prestart": "npm run install-exact",
    "start": "npm run build-cleanup && npm run build-prod",
    "test": "npm run test-unit && npm run test-e2e",
    "test-e2e": "protractor test/icon-e2e.protractor.conf.js",
    "test-unit": "mocha --compilers js:babel-core/register test/unit/**/*.test.js --reporter mocha-jenkins-reporter --reporter-options junit_report_path=./test/icon-unit-test-report.xml",
    "test-unit-silent": "mocha --compilers js:babel-core/register test/unit/**/*.test.js",
    "test-unit-watch": "mocha --compilers js:babel-core/register test/unit/**/*.test.js --watch",
    "prepush": "npm run test-unit-silent",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.config.js"
  }
}
